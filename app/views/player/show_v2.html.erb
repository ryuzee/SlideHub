(function() {
var elm = document.getElementById("slidehub-<%= @prefix %>");
elm.innerHTML = `<%= raw @body.gsub!(/\n/, '') %>`;
var s = document.createElement('script');
<% require 'uglifier'; sc = Uglifier.new(:harmony => true).compile(@script) %>
s.innerText = `<%= raw sc %>`;
s.textContent = `<%= raw sc %>`;
elm.appendChild(s);
})();